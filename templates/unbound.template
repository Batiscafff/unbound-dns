server:
  logfile: "/var/log/unbound/unbound.log"
  verbosity: 1
  log-time-ascii: yes
  log-queries: yes
  interface: 0.0.0.0
  port: 53
  do-ip4: yes
  do-ip6: no
  do-udp: yes
  do-tcp: yes
  access-control: 0.0.0.0/0 allow
  root-hints: "/var/lib/unbound/root.hints"
  hide-identity: yes
  hide-version: yes
  harden-glue: yes
  harden-dnssec-stripped: yes
  use-caps-for-id: yes
  edns-buffer-size: 1232
  prefetch: yes
  cache-min-ttl: 3600
  cache-max-ttl: 86400
  num-threads: 2
  so-rcvbuf: 4m
  so-sndbuf: 4m
  rrset-roundrobin: yes
  minimal-responses: yes
  qname-minimisation: yes
  username: unbound
